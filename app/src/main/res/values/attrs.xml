<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingView">
        <attr name="text" format="string" />
        <attr name="imageResource" format="integer" />
        <attr name="status" >
            <enum name="LOADING" value="0"></enum>
            <enum name="EMPTY" value="1"></enum>
            <enum name="ERROR" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>